# 服务器配置
PORT=9702

# MySQL 数据库配置
DB_HOST=localhost
DB_PORT=3306
DB_NAME=inventory_master
DB_USER=root
DB_PASSWORD=your_database_password

# JWT 配置
JWT_SECRET=your-secret-key-change-this-in-production
JWT_EXPIRES_IN=7d

# 腾讯云 COS 配置
COS_SECRET_ID=your_cos_secret_id
COS_SECRET_KEY=your_cos_secret_key
COS_BUCKET=your-bucket-name
COS_REGION=ap-guangzhou

# Redis 配置
REDIS_HOST=your_redis_host
REDIS_PORT=6379
REDIS_PASSWORD=your_redis_password

# SMTP 邮箱配置
SMTP_HOST=your_smtp_host
SMTP_PORT=465
SMTP_SECURE=true
SMTP_USER=your_smtp_user
SMTP_PASSWORD=your_smtp_password
SMTP_FROM_NAME=洋芋田收纳大师
SMTP_FROM_EMAIL=your_smtp_from_email

# 腾讯云短信配置
SMS_SDK_APP_ID=your_sms_app_id
SMS_TEMPLATE_ID=your_sms_template_id

# 微信开放平台网站应用配置（与站群使用相同配置以实现用户互通）
WECHAT_WEBSITE_APPID=your_wechat_appid
WECHAT_WEBSITE_SECRET=your_wechat_secret

# QQ互联网站应用配置（与站群使用相同配置以实现用户互通）
QQ_WEBSITE_APPID=your_qq_appid
QQ_WEBSITE_SECRET=your_qq_secret

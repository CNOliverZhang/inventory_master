{
  "name": "material-management-backend",
  "version": "1.0.0",
  "description": "个人物资管理系统后端",
  "main": "dist/index.js",
  "scripts": {
    "dev": "nodemon --exec ts-node src/index.ts",
    "build": "tsc",
    "start": "node dist/index.js",
    "pm2:start": "pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop inventory-master-backend",
    "pm2:restart": "pm2 restart inventory-master-backend",
    "pm2:reload": "pm2 reload inventory-master-backend",
    "pm2:delete": "pm2 delete inventory-master-backend",
    "pm2:logs": "pm2 logs inventory-master-backend",
    "pm2:status": "pm2 status inventory-master-backend",
    "deploy": "npm run build && npm run pm2:reload",
    "test:email": "ts-node src/scripts/testEmail.ts",
    "db:fix": "ts-node src/scripts/fixDatabase.ts",
    "check:env": "bash check-server-env.sh"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.13.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cos-nodejs-sdk-v5": "^2.12.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ioredis": "^5.3.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^3.6.5",
    "nodemailer": "^6.9.7",
    "sequelize": "^6.35.2",
    "sharp": "^0.34.5",
    "svg-captcha": "^1.4.0",
    "tencentcloud-sdk-nodejs": "^4.1.149",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.10.6",
    "@types/nodemailer": "^6.4.14",
    "@types/sharp": "^0.32.0",
    "@types/uuid": "^9.0.7",
    "nodemon": "^3.0.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
